<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:remotecontrol="http://schemas.android.com/apk/res/com.axcoto.shinjuku.sushi"
    android:id="@+id/TableLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TableRow
        android:id="@+id/tableRow2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <com.axcoto.shinjuku.sushi.RemoteKeyButton
            android:id="@+id/imageButton1"
            style="@style/RemoteButtonLeft"
            android:onClick="onRemoteClick"
            android:src="@drawable/glyphicons_172_rewind"
            remotecontrol:remote_key_name="test cu" />

        <com.axcoto.shinjuku.sushi.RemoteKeyButton
            android:id="@+id/imageButton2"
            style="@style/RemoteButton"
            android:onClick="onRemoteClick"
            android:src="@drawable/glyphicons_173_play"
            remotecontrol:remote_key_name="cu hai" />

        <com.axcoto.shinjuku.sushi.RemoteKeyButton
            android:id="@+id/imageButton3"
            style="@style/RemoteButton"
            android:src="@drawable/glyphicons_175_stop" />

        <com.axcoto.shinjuku.sushi.RemoteKeyButton
            android:id="@+id/imageButton4"
            style="@style/RemoteButtonRight"
            android:src="@drawable/glyphicons_176_forward" />
    </TableRow>

    <TableRow
        android:id="@+id/tableRow3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <com.axcoto.shinjuku.sushi.RemoteKeyButton
            android:id="@+id/imageButton5"
            style="@style/RemoteButtonLeft"
            android:src="@drawable/glyphicons_100_font" />

        <com.axcoto.shinjuku.sushi.RemoteKeyButton
            android:id="@+id/imageButton6"
            style="@style/RemoteButton"
            android:src="@drawable/ceenee_10_slow" />

        <com.axcoto.shinjuku.sushi.RemoteKeyButton
            android:id="@+id/imageButton7"
            style="@style/RemoteButton"
            android:src="@drawable/glyphicons_085_repeat" />

        <com.axcoto.shinjuku.sushi.RemoteKeyButton
            android:id="@+id/imageButton8"
            style="@style/RemoteButtonRight"
            android:src="@drawable/glyphicons_049_star" />
    </TableRow>

    <TableRow
        android:id="@+id/tableRow4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <com.axcoto.shinjuku.sushi.RemoteKeyButton
            android:id="@+id/imageButton9"
            style="@style/RemoteButtonLeft"
            android:src="@drawable/glyphicons_349_fullscreen" />

        <com.axcoto.shinjuku.sushi.RemoteKeyButton
            android:id="@+id/imageButton10"
            style="@style/RemoteButton"
            android:src="@drawable/glyphicons_279_tablet" />

        <com.axcoto.shinjuku.sushi.RemoteKeyButton
            android:id="@+id/imageButton11"
            style="@style/RemoteButton"
            android:src="@drawable/glyphicons_280_settings" />

        <com.axcoto.shinjuku.sushi.RemoteKeyButton
            android:id="@+id/imageButton12"
            style="@style/RemoteButtonRight"
            android:src="@drawable/glyphicons_180_facetime_video" />
    </TableRow>

    <TableRow
        android:id="@+id/tableRow5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <com.axcoto.shinjuku.sushi.RemoteKeyButton
            android:id="@+id/imageButton14"
            style="@style/RemoteButtonLeft"
            android:src="@drawable/ceenee_11_red" />

        <com.axcoto.shinjuku.sushi.RemoteKeyButton
            android:id="@+id/imageButton13"
            style="@style/RemoteButton"
            android:src="@drawable/ceenee_14_green" />

        <com.axcoto.shinjuku.sushi.RemoteKeyButton
            android:id="@+id/imageButton15"
            style="@style/RemoteButton"
            android:src="@drawable/ceenee_13_yellow" />

        <com.axcoto.shinjuku.sushi.RemoteKeyButton
            android:id="@+id/imageButton16"
            style="@style/RemoteButtonRight"
            android:src="@drawable/ceenee_12_blue" />
    </TableRow>

</TableLayout>